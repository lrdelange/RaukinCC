<Scripts>
 <OnMouseDown>
  if button == "LeftButton" and not self.isMoving then
   self:StartMoving();
   self.isMoving = true;
  end
 </OnMouseDown>
 <OnMouseUp>
  if button == "LeftButton" and self.isMoving then
   self:StopMovingOrSizing();
   self.isMoving = false;
  end
 </OnMouseUp>
 <OnHide>
  if ( this.isMoving ) then
   this:StopMovingOrSizing();
   this.isMoving = false;
  end
 </OnHide>
<OnLoad>RaukinCC_OnLoad();
</OnLoad>
<OnEvent>RaukinCC_OnEvent(self, event, ...);
</OnEvent>
